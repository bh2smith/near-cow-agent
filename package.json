{
  "name": "bitte-cowswap-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "api": "next dev",
    "dev": "concurrently \"next dev\" \"make-agent dev\"",
    "dev-testnet": "concurrently \"next dev\" \"make-agent dev -t\"",
    "build": "next build",
    "deploy": "make-agent deploy -u https://near-cow-agent.vercel.app/",
    "deploy-staging": "make-agent deploy -u https://near-cow-agent-git-staging-bitteprotocol.vercel.app/",
    "start": "next start",
    "lint": "biome check && prettier --check '{src,tests}/**/*.{js,jsx,ts,tsx}'",
    "fmt": "biome format --write . && prettier --write '{src,tests}/**/*.{js,jsx,ts,tsx}'"
  },
  "dependencies": {
    "@bitte-ai/agent-sdk": "^0.4.1",
    "@cowprotocol/cow-sdk": "7.1.1",
    "@cowprotocol/sdk-viem-adapter": "0.2.0",
    "json-stringify-deterministic": "^1.0.12",
    "next": "^15.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "redoc": "^2.5.2",
    "vercel-url": "^0.2.8",
    "viem": "^2.39.0",
    "zerion-sdk": "^0.1.8",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bun": "^1.3.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "biome": "^0.3.3",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "make-agent": "^0.4.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
